{
  "name": "memory-game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^12.20.26",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "classnames": "^2.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "normalize.css": "^8.0.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sass": "^1.42.1",
    "typescript": "^4.4.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{css,scss,json,md,html}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "endOfLine": "auto",
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  },
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prefer-const": "warn",
      "no-var": "warn",
      "@typescript-eslint/no-unused-vars": "error",
      "prettier/prettier": "warn",
      "spaced-comment": [
        "warn",
        "always",
        {
          "markers": [
            "/"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
